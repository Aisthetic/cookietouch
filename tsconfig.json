{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "removeComments": true,
    "sourceMap": true,
    "jsx": "react",
    "noImplicitAny": true,
    "outDir": "./build",
    "baseUrl": "./src",
    "paths": {
      "@account": ["cookie/Account.ts"],
      "@protocol/*": ["cookie/protocol/*"]
    }
  },
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
