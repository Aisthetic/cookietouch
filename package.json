{
  "name": "cookietouch",
  "version": "0.0.1",
  "description": "DofusTouch Bot with Electron React/TypeScript",
  "main": "src/main/index.ts",
  "scripts": {
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist -- --dir",
    "dev": "electron-webpack dev",
    "lint": "tslint -c tslint.json 'src/**/*.tsx' 'src/**/*.ts' -t stylish",
    "lint:fix": "tslint -c tslint.json 'src/**/*.tsx' 'src/**/*.ts' -t stylish --fix",
    "postinstall": "electron-builder install-app-deps",
    "release": "yarn compile && build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yovanoc/cookietouch.git"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "webpack",
    "cookie",
    "cookietouch"
  ],
  "author": "Christopher Yovanovitch <yovano_c@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yovanoc/cookietouch/issues"
  },
  "homepage": "https://github.com/yovanoc/cookietouch#readme",
  "resolutions": {
    "webpack-sources": "1.0.1"
  },
  "devDependencies": {
    "@types/jquery": "^3.2.16",
    "@types/node": "^9.3.0",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "awesome-typescript-loader": "^3.4.0",
    "electron": "^1.7.9",
    "electron-builder": "^19.46.4",
    "electron-webpack": "^1.11.0",
    "electron-webpack-ts": "^1.2.0",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-build-notifier": "^0.1.17"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "4.0.0-beta.3",
    "electron-log": "^2.2.11",
    "electron-updater": "^2.18.2",
    "linqts": "^1.10.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "reactstrap": "^5.0.0-alpha.4",
    "source-map-support": "^0.5.0"
  }
}
