{
  "name": "cookietouch",
  "version": "0.0.2",
  "description": "DofusTouch Bot with Electron React/TypeScript",
  "main": "compiled/electron/main.js",
  "scripts": {
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist -- --dir -c.compression=store -c.mac.identity=null",
    "dev": "electron-webpack dev",
    "lint": "tslint -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.tsx' 'src/**/*.ts' --fix",
    "rebuild-deps": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "me.devchris.cookietouch",
    "mac": {
      "category": "your.app.category.type"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yovanoc/cookietouch.git"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "webpack",
    "cookie",
    "cookietouch"
  ],
  "author": "Christopher Yovanovitch <yovano_c@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yovanoc/cookietouch/issues"
  },
  "homepage": "https://github.com/yovanoc/cookietouch#readme",
  "resolutions": {
    "webpack-sources": "1.0.1"
  },
  "devDependencies": {
    "@types/jquery": "^3.2.16",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "awesome-typescript-loader": "^3.4.0",
    "electron": "^1.8.1",
    "electron-builder": "^19.46.4",
    "electron-webpack": "^1.11.0",
    "electron-webpack-ts": "^1.2.0",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-build-notifier": "^0.1.17"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "4.0.0-beta.2",
    "jquery": "^3.2.1",
    "linqts": "^1.10.0",
    "moment": "^2.19.2",
    "popper.js": "^1.12.9",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "source-map-support": "^0.5.0"
  }
}
