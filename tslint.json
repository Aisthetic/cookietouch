{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "no-eval": false,
    "no-bitwise": false,
    "variable-name": [
      true, "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-snake-case"
    ],
    "indent": [true, "spaces", 2],
    "max-line-length": [true, 160],
    "max-classes-per-file": 2,
    "no-empty-interface": false,
    "quotemark": [true, "double", "avoid-escape"]
  },
  "rulesDirectory": []
}
